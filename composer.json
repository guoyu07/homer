{
    "name": "lawoole/homer",
    "description": "An amazing open source RPC framework based on Swoole",
    "keywords": ["homer", "rpc", "swoole", "lawoole", "framework"],
    "license": "MIT",
    "homepage": "https://github.com/lawoole/homer",
    "type": "library",
    "support": {
        "issues": "https://github.com/lawoole/homer/issues",
        "source": "https://github.com/lawoole/homer"
    },
    "authors": [
        {
            "name": "You Ming",
            "email": "youming@funcuter.org"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/container": "~5.4",
        "illuminate/support": "~5.4",
        "monolog/monolog": "~1.20"
    },
    "require-dev": {
        "lawoole/swoole-api": "dev-master",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "psr-4": {
            "Homer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Homer\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
